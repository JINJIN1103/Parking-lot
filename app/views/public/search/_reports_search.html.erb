<table class='table'>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% reports.each do |report| %>
      <tr>
        <td><%= attachment_image_tag(report, :image, :fill, 50, 50, fallback: "no_image.jpg") %></td>
        <td><%= report.title_r %></td>
        <td><%= link_to 'Show',report_path(report) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
